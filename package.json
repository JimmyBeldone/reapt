{
    "name": "reapt",
    "version": "1.8.9",
    "description": "A complete React Redux starterkit, configured with Webpack, stop wasting time configuring, adopt it and just code",
    "main": "src/index.js",
    "scripts": {
        "setup": "node webpack/setup/setupMessage.js && yarn && node webpack/setup/setup.js",
        "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.development.js --hot",
        "build": "yarn clean-dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.production.js --progress --hide-modules",
        "build:srv": "cross-env NODE_ENV=production http-server dist -g -p 4000 -o",
        "analyze": "yarn clean-dist && cross-env NODE_ENV=production ADDONS=bundleanalyze webpack",
        "clean-dist": "yarn remove-dist && mkdir dist",
        "pretty": "prettier --find-config-path '.prettierrc' && prettier --write 'src/**/*.js'",
        "lint": "eslint src webpack --fix",
        "stylint": "stylint src",
        "commit": "git-cz",
        "release": "standard-version",
        "post-release": "git push --follow-tags origin",
        "remove-dist": "rimraf ./dist",
        "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js",
        "webpack": "node ./node_modules/webpack/bin/webpack.js",
        "webpack-defaults": "webpack-defaults",
        "mk": "node ./node_modules/mk-react-comp/lib/index.js"
    },
    "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/JimmyBeldone/reapt"
    },
    "bugs": {
        "email": "dev.jimmy.beldone@gmail.com",
        "url": "https://github.com/JimmyBeldone/reapt/issues"
    },
    "keywords": [
        "react",
        "boilerplate",
        "starterkit",
        "starter kit",
        "webpack",
        "hmr",
        "stylus",
        "redux",
        "intl",
        "code splitting",
        "mk-react-comp"
    ],
    "license": "MIT",
    "browserslist": {
        "production": [
            "> 1%",
            "last 2 versions",
            "Firefox ESR"
        ],
        "legacyBrowsers": [
            "> 1%",
            "last 2 versions",
            "Firefox ESR"
        ],
        "modernBrowsers": [
            "last 2 Chrome versions",
            "not Chrome < 60",
            "last 2 Safari versions",
            "not Safari < 10.1",
            "last 2 iOS versions",
            "not iOS < 10.3",
            "last 2 Firefox versions",
            "not Firefox < 54",
            "last 2 Edge versions",
            "not Edge < 15"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "*.(js|jsx)": [
            "yarn lint",
            "git add"
        ],
        "*.(styl|css)": [
            "yarn stylint",
            "git add"
        ]
    },
