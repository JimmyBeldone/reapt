{
    "name": "reapt",
    "version": "1.1.2",
    "description": "A complete React Redux starterkit, configured with Webpack, stop wasting time configuring, adopt it and just code",
    "main": "src/index.js",
    "scripts": {
        "setup": "node webpack/setup/setupMessage.js && yarn && node webpack/setup/setup.js",
        "start": "cross-env NODE_ENV=development webpack-dev-server --hot",
        "dashboard": "cross-env WITH_DASHBOARD=true webpack-dashboard -- yarn start",
        "build": "yarn clean-dist && cross-env NODE_ENV=production webpack",
        "build:srv": "babel-node webpack/distServer.js",
        "analyze": "yarn clean-dist && cross-env NODE_ENV=production ADDONS=bundleanalyze webpack",
        "clean-dist": "yarn remove-dist && mkdir dist",
        "lint": "eslint src webpack --fix",
        "remove-dist": "rimraf ./dist",
        "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js",
        "webpack": "node ./node_modules/webpack/bin/webpack.js",
        "webpack-defaults": "webpack-defaults",
        "mk": "node ./node_modules/mk-react-comp/lib/index.js"
    },
    "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/JimmyBeldone/reapt"
    },
    "keywords": [
        "react",
        "boilerplate",
        "starterkit",
        "starter kit",
        "webpack",
        "hmr",
        "stylus",
        "redux",
        "intl",
        "code splitting",
        "mk-react-comp"
    ],
    "license": "MIT",
    "dependencies": {
